{
  "name": "shopify-app-node",
  "private": true,
  "scripts": {
    "build": "npm run build:client",
    "build:client": "vite build --outDir dist/client",
    "debug": "node --inspect-brk server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch ./server",
    "prepare": "husky install",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production node server/index.js",
    "start": "npm run serve",
    "test": "vitest --reporter=verbose",
    "server": "nodemon server/server.js --watch ./server"
  },
  "type": "module",
  "engines": {
    "node": ">=16.13.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@mui/material": "^5.11.15",
    "@mui/styled-engine-sc": "^5.11.11",
    "@rpldy/uploader": "^1.2.0",
    "@rpldy/uploady": "^1.2.0",
    "@sendgrid/mail": "^7.7.0",
    "@shopify/app-bridge": "^3.1.0",
    "@shopify/app-bridge-react": "^3.1.0",
    "@shopify/app-bridge-utils": "^3.1.0",
    "@shopify/polaris": "^9.11.0",
    "@shopify/polaris-icons": "^4.19.0",
    "@shopify/shopify-api": "^3.1.0",
    "@vitejs/plugin-react": "1.2.0",
    "aws-sdk": "^2.1366.0",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.3",
    "exit-hook": "^3.0.0",
    "express": "^4.17.3",
    "formidable": "^2.0.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "mongoose": "^6.3.0",
    "multer": "^1.4.5-lts.1",
    "query-string": "^7.1.1",
    "raw-body": "^2.5.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.3",
    "react-grid": "^4.0.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-toggle": "^4.1.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "serve-static": "^1.14.1",
    "store2": "^2.13.2",
    "styled-components": "^5.3.9",
    "vite": "^2.9.1"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.2",
    "vitest": "^0.9.2"
  }
}
